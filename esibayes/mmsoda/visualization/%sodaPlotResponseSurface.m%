function sodaPlotResponseSurface(sodaPar,evalResults,fixedValue)

M=evalResults(:,sodaPar.parCols);
%fixedValue = [10.5,NaN,NaN];

IxPars = find(isnan(fixedValue));
p1 = IxPars(1)+1;
p2 = IxPars(2)+1;
clear IxPars


Ix=find(sum(M==repmat(fixedValue,[size(evalResults,1),1]),2)==(sodaPar.nOptPars-2))

nPixelsPerAxis = numel(Ix)^(1/2);

X = reshape(evalResults(Ix,p1),[nPixelsPerAxis,nPixelsPerAxis])
Y = reshape(evalResults(Ix,p2),[nPixelsPerAxis,nPixelsPerAxis])
Z = reshape(evalResults(Ix,sodaPar.objCol),[nPixelsPerAxis,nPixelsPerAxis])


xVec = X(1,:);
yVec = Y(:,1)';

imagesc(xVec,yVec,Z)
xlabel(sodaPar.parNames{p1-1})
ylabel(sodaPar.parNames{p2-1})